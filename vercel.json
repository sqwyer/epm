{
    "version": 2,
    "builds": [
        {
          "src": "server/dist/**.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["server/dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/dist/index.js"
        }
    ]
}